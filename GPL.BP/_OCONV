$internal
$no.symbols
$no.xref
$recursive

subroutine oconv(src, conv)
$include syscom.h

   subr.name = field(conv,'.',1)
   if not(catalogued(subr.name)) then
      set.status 2
      return value src
   end

   at.conv = field(conv,'.',2,9999)
   result = src
   state = 1
   call @subr.name(result, src, state, @true)
   set.status state

   return value result
end

* END-CODE
