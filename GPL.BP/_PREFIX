$internal
$no.symbols
$no.xref
$recursive

subroutine prefix(unit, pathname)

$include keys.h

   openseq pathname to pfx.f then
      loop
         readblk pfx from pfx.f, 1024 else exit
         print on unit pfx :
      repeat
   end

   return
end


* END-CODE
